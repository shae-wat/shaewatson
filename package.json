{
  "name": "shaewatson",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/jest": "24.0.18",
    "@types/node": "12.7.12",
    "@types/react": "16.9.5",
    "@types/react-dom": "16.9.1",
    "lodash": "^4.17.15",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-scripts": "3.2.0",
    "typescript": "3.6.4"
  },
  "scripts": {
    "make-prettier": "prettier --config .prettierrc '**/*.{ts,tsx}' --write",
    "check-prettier": "prettier --config .prettierrc '**/*.{ts,tsx}' --list-different ",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "yarn run check-prettier && react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "collectCoverageFrom": [
      "!**/*.d.ts",
      "src/**/*.{ts,tsx}",
      "!<rootDir>/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-push": "CI=true yarn test"
    }
  },
  "browserslist": [
    "last 4 major versions",
    "not dead",
    "not ie < 11"
  ],
  "devDependencies": {
    "@types/lodash": "^4.14.146",
    "@types/styled-components": "^4.1.19",
    "husky": "^3.0.9",
    "prettier": "^1.18.2",
    "styled-components": "^4.4.0"
  }
}
